{
    "name": "<%= projectNamespacedName %>",
    "version": "<%= projectVersion %>",
    "description": "<%= projectDescription %>",
    "main": "dist/index.js",
    "scripts": {
        "all": "gulp --series format lint build test-unit clean",
        "test": "gulp --series build test-unit",
        "package": "gulp --series clean lint format build test-unit package",
        "publish": "gulp --series clean lint format build test-unit package publish",
        "watch:build": "gulp watch-build",
        "watch:test": "gulp watch-test-unit",
        "start": "node src/index",
        "start:dev": "dotenv-load node src/index"
    },
    "files": [ "package.json", "LICENSE", "README.md", "src/**/*" ],
    "author": "<%= authorName %> <<%= authorEmail %>>",
    "keywords": <%- JSON.stringify(projectKeywords) %>,
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.7",
        "@types/chai-as-promised": "^7.1.2",
        "@types/express": "^4.17.2",
        "@types/mocha": "^5.2.7",
        "@types/sinon-chai": "^3.2.3",
        "@vamship/build-utils": "0.2.0",
        "@vamship/test-utils": "^2.4.9",
        "ansi-colors": "^4.1.1",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "fancy-log": "^1.3.3",
        "chai-http": "^4.3.0",
        "dotenv-load": "^2.0.0",
        "gulp": "^4.0.2",
        "mocha": "^7.0.0",
        "nyc": "^15.0.0",
        "rewire": "^4.0.1",
        "sinon": "^8.1.0",
        "sinon-chai": "^3.4.0",
        "sinon-express-mock": "^2.2.1",
        "tslint": "^5.20.1",
        "tslint-no-unused-expression-chai": "^0.1.4",
        "typedoc": "^0.16.7",
        "typescript": "^3.7.5"
    },
    "dependencies": {
        "@vamship/arg-utils": "^2.4.6",
        "@vamship/error-types": "^1.7.8",
        "@vamship/expressjs-routes": "^1.7.2",
        "@vamship/logger": "^4.2.6",
        "ajv": "^6.11.0",
        "bluebird": "^3.7.2",
        "express": "^4.17.1"
    },
    "repository": {
        "type": "git",
        "url": "https://<%= gitUrl %>.git"
    },
    "bugs": {
        "url": "https://<%= gitUrl %>/issues"
    },
    "homepage": "https://<%= gitUrl %>#readme",
    "buildMetadata": {
        "projectType": "api",
        "requiredEnv": [],
<% if(dockerRequired) {-%>
        "language": "ts",
        "docker": {
            "default": {
                "repo": "<%= dockerFullRepo %>",
                "buildArgs": {
                }
            }
        }
<% } else {-%>
        "language": "ts"
<% }-%>
    }
}
